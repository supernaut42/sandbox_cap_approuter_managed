<mvc:View
    controllerName="sandbox.app.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <List
                headerText="{i18n>listTitle}"
                headerLevel="H2"
                items="{
			path: '/DummyEntity'
		}"
            >
                <StandardListItem
                    title="{someText}"
                    counter="{someNumber}"
                />
            </List>
        </content>
    </Page>
</mvc:View>
